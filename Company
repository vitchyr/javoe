public class Company 
{
	private int id;
	private int imageId;
	private String name;
	
	public Company(){}
	
	public Company(int id, int imageId, String name)
	{
		this.id = id;
		this.imageId = imageId;
		this.name = name;
	}
	public void setId(int id)
	{
		this.id = id;
	}
	public int getId()
	{
		return id;
	}
	public void setImageId(int imageId)
	{
		this.imageId = imageId;
	}
	public int getImageId()
	{
		return imageId;
	}
	public void setName(String name)
	{
		this.name = name;
	}
	public String getName()
	{
		return name;
	}
}
