public class Food 
{
	private int id;
	private int imageId;
	private String name;
	private int protein;
	private int sugars;
	private int diet_fiber;
	private int carb;
	private int sodium;
	private int chol;
	private int trans_fat;
	private int calories_from_fat;
	private int saturated_fat;
	private int total_fat;
	private int calories;

	public Food(){}
	
	public Food(int id, int imageId, String name, int protein, int sugars, int diet_fiber, int carb, int sodium, int chol, int trans_fat, int calories_from_fat, int saturated_fat, int total_fat, int calories)
	{
		this.id = id;
		this.imageId = imageId;
		this.name = name;
		this.protein = protein;
		this.sugars = sugars;
		this.diet_fiber = diet_fiber;
		this.carb = carb;
		this.sodium = sodium;
		this.chol = chol;
		this.trans_fat = trans_fat;
		this.calories_from_fat = calories_from_fat;
		this.saturated_fat = saturated_fat;
		this.total_fat = total_fat;
		this.calories = calories;
	}
	
	public void setId(int id)
	{
		this.id = id;
	}
	public int getId()
	{
		return id;
	}
	public void setImageId(int imageId)
	{
		this.imageId = imageId;
	}
	public int getImageId()
	{
		return imageId;
	}
	public void setName(String name)
	{
		this.name = name;
	}
	public String getName()
	{
		return name;
	}
	public void setProtein(int protein)
	{
		this.protein = protein;
	}
	public int getProtein()
	{
		return protein;
	}
	public void setSugar(int sugars)
	{
		this.sugars = sugars;
	}
	public int getSugar()
	{
		return sugars;
	}
	public void setDiet_Fiber(int diet_fiber)
	{
		this.diet_fiber = diet_fiber;
	}
	public int getDiet_Fiber()
	{
		return diet_fiber;
	}
	public void setCarb(int carb)
	{
		this.carb = carb;
	}
	public int getCarb()
	{
		return carb;
	}
	public void setSodium(int sodium)
	{
		this.sodium = sodium;
	}
	public int getSodium()
	{
		return sodium;
	}
	public void setCholestrol(int chol)
	{
		this.chol = chol;
	}
	public int getCholestrol()
	{
		return chol;
	}
	public void setTrans_Fat(int trans_fat)
	{
		this.trans_fat = trans_fat;
	}
	public int getTrans_Fat()
	{
		return trans_fat;
	}
	public void setCalories_from_fat(int calories_from_fat)
	{
		this.calories_from_fat = calories_from_fat;
	}
	public int getCalories_from_fat()
	{
		return calories_from_fat;
	}
	public void setSaturated_fat(int saturated_fat)
	{
		this.saturated_fat = saturated_fat;
	}
	public int getSaturated_fat()
	{
		return saturated_fat;
	}
	public void setTotal_fat(int total_fat)
	{
		this.total_fat = total_fat;
	}
	public int getTotal_fat()
	{
		return total_fat;
	}
	public void setCalories(int calories)
	{
		this.calories = calories;
	}
	public int getCalories()
	{
		return calories;
	}
}
